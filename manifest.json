{
    "manifest_version": 2,
    "name": "YouTube Speedy",
    "version": "1.0",
    "description": "Increase YouTube playback speed beyond 2x.",
    "permissions": ["activeTab"],
    "browser_action": {
      "default_popup": "popup.html",
      "default_icon": {
        "48": "icons/logo.webp"
      }
    },
    "content_scripts": [
      {
        "matches": ["*://*.youtube.com/*"],
        "js": ["content_script.js"]
      }
    ],
    "icons": {
        "48": "icons/logo.webp"
    }
  }
  